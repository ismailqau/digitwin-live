{
  "name": "digitwinlive",
  "version": "1.0.0",
  "private": true,
  "description": "DigiTwin Live - Real-Time Conversational AI System - Monorepo",
  "workspaces": [
    "apps/*",
    "services/*",
    "packages/*"
  ],
  "scripts": {
    "audit:check": "pnpm audit --audit-level=moderate",
    "build": "turbo run build",
    "cache:cleanup": "ts-node scripts/cache-cleanup.ts",
    "cache:stats": "ts-node scripts/cache-stats.ts",
    "clean": "turbo run clean && rimraf node_modules",
    "complexity:check": "npx madge --circular --extensions ts,tsx .",
    "coverage:check": "pnpm test:coverage && npx nyc check-coverage --lines 80",
    "db:generate": "cross-env DATABASE_URL=$DATABASE_URL pnpm --filter @clone/database prisma:generate",
    "db:migrate": "pnpm --filter @clone/database prisma:migrate",
    "db:migrate:deploy": "pnpm --filter @clone/database prisma:migrate:deploy",
    "db:reset": "pnpm --filter @clone/database prisma:reset",
    "db:seed": "pnpm --filter @clone/database prisma:seed",
    "db:studio": "pnpm --filter @clone/database prisma:studio",
    "dev": "./scripts/dev.sh",
    "dev:turbo": "turbo run dev",
    "docs:verify": "./scripts/verify-docs.sh",
    "fix": "pnpm lint:fix && pnpm format && pnpm sort-package-json",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "gcp:cleanup": "./scripts/gcp-cleanup.sh",
    "gcp:cleanup-all": "./scripts/gcp-cleanup.sh --all",
    "gcp:cleanup-menu": "./scripts/gcp-cleanup.sh --menu",
    "gcp:cleanup-selective": "./scripts/gcp-cleanup.sh --selective",
    "gcp:cleanup-sql": "./scripts/cleanup-sql-only.sh",
    "gcp:cost": "./scripts/gcp-manage.sh cost",
    "gcp:create-sql": "./scripts/gcp-create-sql.sh",
    "gcp:deploy": "./scripts/gcp-deploy.sh deploy",
    "gcp:deploy:api-gateway": "./scripts/gcp-deploy.sh deploy api-gateway",
    "gcp:deploy:face": "./scripts/gcp-deploy.sh deploy face-processing-service",
    "gcp:deploy:websocket": "./scripts/gcp-deploy.sh deploy websocket-server",
    "gcp:list": "./scripts/gcp-manage.sh list",
    "gcp:services:delete": "./scripts/gcp-deploy.sh delete",
    "gcp:services:status": "./scripts/gcp-deploy.sh status",
    "gcp:setup": "./scripts/gcp-setup.sh",
    "gcp:start-all": "./scripts/gcp-manage.sh start-all",
    "gcp:status": "./scripts/gcp-manage.sh status",
    "gcp:stop-all": "./scripts/gcp-manage.sh stop-all",
    "health:vector-db": "node scripts/health-check-vector-db.js",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint -- --fix",
    "outdated:check": "pnpm outdated --format=list",
    "prepare": "husky install",
    "quality:check": "./scripts/quality-check.sh",
    "setup:all": "./scripts/setup-all.sh",
    "setup:quality": "./scripts/setup-quality-gates.sh",
    "setup:tests": "./scripts/setup-tests.sh",
    "sort-package-json": "sort-package-json package.json apps/*/package.json services/*/package.json packages/*/package.json",
    "test": "turbo run test",
    "test:changed": "turbo run test --filter=[HEAD^1]",
    "test:coverage": "turbo run test -- --coverage",
    "test:face-processing:gcp": "./scripts/test-face-processing-gcp.sh",
    "test:face-processing:integration": "./scripts/test-face-processing-integration.sh",
    "test:face-processing:local": "./scripts/test-face-processing-local.sh",
    "test:gcp": "./scripts/gcp-vector-db-test.sh",
    "test:monitoring": "./scripts/test-monitoring.sh",
    "test:watch": "turbo run test -- --watch",
    "tf:apply": "./scripts/terraform-manage.sh apply",
    "tf:apply-monitoring": "./scripts/terraform-manage.sh apply-monitoring",
    "tf:fmt": "./scripts/terraform-manage.sh fmt",
    "tf:init": "./scripts/terraform-manage.sh init",
    "tf:init-local": "./scripts/terraform-manage.sh init-local",
    "tf:plan": "./scripts/terraform-manage.sh plan",
    "tf:plan-monitoring": "./scripts/terraform-manage.sh plan-monitoring",
    "tf:validate": "./scripts/terraform-manage.sh validate",
    "type-check": "turbo run type-check",
    "validate": "pnpm lint && pnpm type-check && pnpm test && pnpm build",
    "verify:local": "./scripts/verify-local-vector-db.sh",
    "verify:setup": "./scripts/verify-setup.sh",
    "verify:vector-db": "node scripts/verify-vector-db.js"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@eslint/js": "^9.38.0",
    "@testing-library/react-native": "^12.4.3",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "cross-env": "^7.0.3",
    "dotenv": "^17.2.3",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.32.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-expo": "^54.0.13",
    "lint-staged": "^15.2.0",
    "madge": "^6.1.0",
    "markdownlint-cli": "^0.38.0",
    "pg": "^8.16.3",
    "prettier": "^3.6.2",
    "react-test-renderer": "^19.1.0",
    "rimraf": "^6.0.1",
    "shx": "^0.3.4",
    "sort-package-json": "^2.6.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "turbo": "^2.5.8",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2"
  },
  "packageManager": "pnpm@8.10.0",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "pnpm": {
    "overrides": {
      "@babel/plugin-proposal-async-generator-functions": ">=7.20.7",
      "@babel/plugin-proposal-class-properties": ">=7.18.6",
      "@babel/plugin-proposal-nullish-coalescing-operator": ">=7.18.6",
      "@babel/plugin-proposal-numeric-separator": ">=7.18.6",
      "@babel/plugin-proposal-object-rest-spread": ">=7.20.7",
      "@babel/plugin-proposal-optional-catch-binding": ">=7.18.6",
      "@babel/plugin-proposal-optional-chaining": ">=7.21.0",
      "abab": ">=2.0.6",
      "are-we-there-yet": ">=4.0.0",
      "body-parser@>=2.2.0 <2.2.1": ">=2.2.1",
      "domexception": ">=4.0.0",
      "flatten": ">=1.0.3",
      "gauge": ">=5.0.0",
      "glob@>=11.0.0 <11.1.0": ">=11.1.0",
      "glob@>=10.2.0 <10.5.0": ">=10.5.0",
      "glob": ">=11.1.0",
      "glob@<8": ">=11.0.0",
      "inflight": "npm:@aashutoshrathi/inflight@^1.0.0",
      "js-yaml@>=4.0.0 <4.1.1": ">=4.1.1",
      "js-yaml@<3.14.2": ">=3.14.2",
      "lodash.get": ">=4.4.2",
      "lodash.isequal": ">=4.5.0",
      "node-domexception": ">=1.0.0",
      "node-forge@<1.3.2": ">=1.3.2",
      "npmlog": ">=7.0.0",
      "react-native>glob": "11.1.0",
      "rimraf@<4": ">=6.0.0",
      "shelljs>glob": "11.1.0",
      "swagger-jsdoc>glob": "11.1.0",
      "tar@7.5.1": ">=7.5.2"
    }
  }
}
